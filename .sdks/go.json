{
    "name": "go",
    "type": "sdk",
    "version": "1.19.3",
    "commands": [
        {
            "type": "rm",
            "target": "/usr/local/go"
        },
        {
            "type": "tar",
            "url": "https://go.dev/dl/go$version.linux-amd64.tar.gz",
            "target": "/usr/local"
        },
        {
            "type": "export",
            "variable": "PATH",
            "value": "/usr/local/go/bin"
        }
    ]
}